#!/usr/bin/env python

import argparse

from torchpack.launch import drun


def main():
    parser = argparse.ArgumentParser()
    parser.add_argument('mode', type=str, choices=['dist-run'])
    args, opts = parser.parse_known_args()

    if args.mode == 'dist-run':
        drun(opts)


if __name__ == '__main__':
    main()
